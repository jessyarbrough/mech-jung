packages:
  yum:
    gcc-c++: []
    gcc-gfortran: []
    python-devel: []
    atlas-sse3-devel: []
    lapack-devel: []
    libpng-devel: []
    freetype-devel: []
    zlib-devel: []
    
container_commands:
  replace_wsgi_config:
    command: "cp .ebextensions/wsgi.conf /opt/python/ondeck/wsgi.conf"
  easy_install_nltk:
    command: "sudo easy_install nltk"
  nltk_download_all:
    command: "python -m nltk.downloader all"
  set_nltk_path:
    command: "export NLTK_DATA=/home/ec2-user/nltk_data"

